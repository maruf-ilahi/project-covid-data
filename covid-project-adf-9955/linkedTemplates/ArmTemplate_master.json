{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "covid-project-adf-9955"
		},
		"LSDymBlob_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LSDymBlob'"
		},
		"ls_covid_source_blob_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_covid_source_blob'"
		},
		"ls_covid_target_adls_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_covid_target_adls'"
		},
		"ls_covid_target_adls_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://covidadlssa.dfs.core.windows.net"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "covid-project-adf-9955_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LSDymBlob_connectionString": {
						"value": "[parameters('LSDymBlob_connectionString')]"
					},
					"ls_covid_source_blob_connectionString": {
						"value": "[parameters('ls_covid_source_blob_connectionString')]"
					},
					"ls_covid_target_adls_accountKey": {
						"value": "[parameters('ls_covid_target_adls_accountKey')]"
					},
					"ls_covid_target_adls_properties_typeProperties_url": {
						"value": "[parameters('ls_covid_target_adls_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}